package praktikumpemrograman1;

/**
 *
 * @author Dina
 */
public class A {
    public static void main (String[] args) {
        
        for( int i = 0; i>=10; i-=2) {
            for( int j = 1; j<=i; j+=2) {
                
                System.out.println(j+"");
            }
             System.out.print("");
        }
    }
}
